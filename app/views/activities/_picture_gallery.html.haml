.carousel-control
  .span-1
    %button.prev <<
  .span-12
    .carousel
      %ul
        - pictures.each do |picture|
          %li=link_to(image_tag(picture.photo.url(:small)), show_gallery_picture_path(picture), :class=>"gallery_pics",:rel=>"gallery")
  .span-1
    %button.next >>
.hide
  #picture-preview